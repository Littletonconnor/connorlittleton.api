{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NODE_ENV"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "lint": {},
    "lint:fix": {},
    "format:check": {},
    "format:write": {},
    "typecheck": {},
    "dev": {
      "cache": false
    }
  }
}
